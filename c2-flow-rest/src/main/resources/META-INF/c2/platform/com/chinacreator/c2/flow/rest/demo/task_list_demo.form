      </form:child>
    </form:child>
  </form:child>
  <form:datasources>
    <form:datasource xsi:type="form:Inputs"/>
  </form:datasources>
  <form:scripts>loadTodoList:function(){

	if($scope.subject||$scope.subject.id){
		
		//查询当前用户(处理人、候选人、所属组)的待办
		var toDoParams={&quot;start&quot;: 0,
					&quot;size&quot;:20,
					&quot;candidateOrAssigned&quot;:$scope.subject.id}
		if($params.userGroups){
			toDoParams.candidateGroupIn=[$params.userGroups];
		}
		
		$http.post(&quot;v1/flow/query/runtime/tasks&quot;,toDoParams).success(function(data){
			$scope.todoList=data.data;
		})
		
	}else{
		Messenger.error(&quot;当前用户id获取为空，无法获取待办列表!&quot;);
	}
},
loadDoneList:function(){
	if($scope.subject||$scope.subject.id){
		$http.post(&quot;v1/flow/query/history/tasks&quot;,{&quot;start&quot;: 0,&quot;size&quot;:20,&quot;taskAssignee&quot;:$scope.subject.id}).success(function(data){
		$scope.doneList=data.data;
	})
	}else{
		Messenger.error(&quot;当前用户id获取为空，无法获取已办列表!&quot;);
	}
},
page_beforeDataSourceLoad:function($params){
	functions.loadTodoList();
	functions.loadDoneList();
},

page_onload:function(){
	
},
openDetail:function(taskId){
	Modal.open(&quot;f/task_detail_demo&quot;,{taskId:taskId},
	    function(AA){
	        functions.loadTodoList();
	    },
	    function(){

	    }
    );
},
openMonitor:function(procinstid){
	Modal.open(&quot;f/task_monitor_demo&quot;,{processInstanceId:procinstid},
	    function(AA){
	        
	    },
	    function(){

	    }
    );
},
button_click:function($event){
	Modal.open(&quot;f/task_add_demo&quot;,{},function(){
		functions.loadTodoList();
	});
}</form:scripts>
  <form:htmls>
&lt;script type=&quot;text/ng-template&quot; id=&quot;div_html&quot;>
&lt;table style=&quot;width:100%;&quot;>
	&lt;tr>
		&lt;th style=&quot;width: 25%;&quot;>流程实例id&lt;/th>
		&lt;th style=&quot;width: 25%;&quot;>任务id&lt;/th>
    	&lt;th style=&quot;width: 10%;&quot;>任务名称&lt;/th>
    	&lt;th>处理人&lt;/th>
	    &lt;th>创建时间&lt;/td>
		&lt;th>状态&lt;/td>
		&lt;th>操作&lt;/th>
		&lt;th>监控&lt;/th>
  	&lt;/tr>

	&lt;tr ng-repeat=&quot;todo in todoList&quot;>
		&lt;td>{{todo.processInstanceId}}&lt;/td>
		&lt;td>{{todo.id}}&lt;/td>
    	&lt;td>{{todo.name}}&lt;/td>
    	&lt;td>{{todo.assignee}}&lt;/td>
		&lt;td>{{todo.createTime|date :'yyyy-MM-dd hh:mm:ss'}}&lt;/td>
		&lt;td>待处理&lt;/td>
		&lt;td>&lt;a href=&quot;javascript:void(0);&quot; ng-click=&quot;functions.openDetail(todo.id)&quot;>详细&lt;/a>&lt;/td>
		&lt;th>&lt;a href=&quot;javascript:void(0);&quot; ng-click=&quot;functions.openMonitor(todo.processInstanceId)&quot;>监控&lt;/a>&lt;/th>
  	&lt;/tr>
&lt;/table>
&lt;/script>
&lt;script type=&quot;text/ng-template&quot; id=&quot;div1_html&quot;>
&lt;table style=&quot;width:100%;&quot;>
	&lt;tr>
		&lt;th  style=&quot;width: 25%;&quot;>流程实例id&lt;/th>
		&lt;th style=&quot;width: 25%;&quot;>任务id&lt;/th>
    	&lt;th style=&quot;width: 10%;&quot;>任务名称&lt;/th>
    	&lt;th>处理人&lt;/th>
	    &lt;th>创建时间&lt;/td>
		&lt;th>状态&lt;/td>
		&lt;th>操作&lt;/th>
		&lt;th>监控&lt;/th>
  	&lt;/tr>

	&lt;tr ng-repeat=&quot;done in doneList&quot;>
		&lt;td>{{done.processInstanceId}}&lt;/td>
		&lt;td>{{done.id}}&lt;/td>
    	&lt;td>{{done.name}}&lt;/td>
    	&lt;td>{{done.assignee}}&lt;/td>
		&lt;td>{{done.startTime|date :'yyyy-MM-dd hh:mm:ss'}}&lt;/td>
		&lt;td>已处理&lt;/td>
		&lt;td>&lt;a href=&quot;javascript:void(0);&quot; ng-click=&quot;functions.openDetail(done.id)&quot;>详细&lt;/a>&lt;/td>
		&lt;th>&lt;a href=&quot;javascript:void(0);&quot; ng-click=&quot;functions.openMonitor(done.processInstanceId)&quot;>监控&lt;/a>&lt;/th>
  	&lt;/tr>
&lt;/table>
&lt;/script></form:htmls>
</form:Form>
