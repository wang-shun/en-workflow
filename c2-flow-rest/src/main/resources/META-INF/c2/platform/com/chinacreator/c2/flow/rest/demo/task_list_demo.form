  <form:child xsi:type="form:Group" id="toolbarWidget" name="标题栏工具条">
    <form:control type="container/toolbar_widget">
      <form:attributes source="container/toolbar_widget"/>
    </form:control>
    <form:child xsi:type="form:Field" id="button" name="创建新流程实例" customize="true">
      <form:control type="widget/button">
