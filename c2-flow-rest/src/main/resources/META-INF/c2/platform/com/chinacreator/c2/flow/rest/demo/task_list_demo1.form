  <form:scripts>page_beforeDataSourceLoad:function($params){
	$scope.todoUrl = {$url:&quot;v1/flow/query/runtime/tasks&quot;};
	$scope.doneUrl={$url:&quot;v1/flow/query/history/tasks&quot;};
},
page_onload:function(){
	if($scope.subject||$scope.subject.id){
		var toDoParam={candidateOrAssigned:$scope.subject.id};
		if($params.userGroups){
			toDoParam.candidateGroupIn=[$params.userGroups];
		}
		$view.tableTodo.refresh(toDoParam);
		
		$view.tableDone.refresh({taskAssignee:$scope.subject.id});
	}else{
		Messenger.error(&quot;当前用户id获取为空，无法获取列表!&quot;);
	}
},
openDetail:function(taskId){
	Modal.open(&quot;f/task_detail_demo&quot;,{taskId:taskId},
	    function(AA){
	        $view.tableTodo.refresh();
	    },
	    function(){

	    }
    );
},
openMonitor:function(procinstid){
	Modal.open(&quot;f/task_monitor_demo&quot;,{processInstanceId:procinstid},
	    function(AA){
	        
	    },
	    function(){

	    }
    );
},
button_click:function($event){
	Modal.open(&quot;f/task_add_demo&quot;,{},function(){
		 $view.tableTodo.refresh();
	});
},

tableJqgrid_beforeInit:function(jqGridOptions){
	jqGridOptions.ajaxGridOptions = { contentType: 'application/json; charset=utf-8' };
	jqGridOptions.jsonReader = {
	    repeatitems: false,
	    id: &quot;id&quot;,
	    root: function (obj) {
	    	return obj.data;
	    },
	    page: function (obj) {
	        //计算当前页
	    	return obj.start/obj.size+1;
	    },
	    total: function (obj){
	    	//计算出总页数
	    	return (obj.total + obj.size -1)/obj.size;
	    },
	    records: function (obj) {
	    	//获取总记录数
	    	return obj.total;
	    }
	};
	jqGridOptions.serializeGridData = function(postData){
		var myPost = {start:(postData.page-1)*postData.rows,size:postData.rows};
 		for (pd in postData){
    		if($.trim(postData[pd])!=&quot;&quot;) myPost[pd]=postData[pd];
    	}
    	
    	//去掉无用的字段
		delete myPost.nd;
		delete myPost._search;
		delete myPost.rows;
		delete myPost.page;
		delete myPost.sidx;
		delete myPost.sord;
		delete myPost.nodeid;
		delete myPost.n_level;
		
		var json = JSON.stringify(myPost);
		return json;
	};
},
colJqgrid6_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	return &quot;&lt;a href=\&quot;javascript:void(0);\&quot; ng-click=\&quot;functions.openDetail(rowObject.id)\&quot;>详细&quot;;
},

colJqgrid7_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	return &quot;&lt;a href=\&quot;javascript:void(0);\&quot; ng-click=\&quot;functions.openMonitor(rowObject.processInstanceId)\&quot;>监控&quot;;
},

colJqgrid5_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	return &quot;待处理&quot;;
},

colJqgrid13_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	return &quot;已处理&quot;;
}</form:scripts>
