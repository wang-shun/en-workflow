  <form:scripts>

page_onload:function(){
	
},

taskType_change:function(){
	this.fieldConfigGroup_loadComplete();
},

appId_change:function(){
	this.fieldConfigGroup_loadComplete();
},

tenantId_change:function(){
	this.fieldConfigGroup_loadComplete();
},

flowEngine_change:function(){
	this.fieldConfigGroup_loadComplete();
},

onlyTitle_onChange:function($event){
	var param = {
		appId:$model.appId,
		engineName:$model.engineName,
		taskType:$model.taskList,
		onlyTitle:$model.onlyTitle
    };
    $http.post(&quot;workflow/uniteconfig/saveconfig&quot;,param).success(function(data,status,headers,config){
    	if(status == 200){
			Messenger.success('保存成功');
    	}else{
			Messenger.error('保存失败');
    	}
	});
},

fieldConfigGroup_loadComplete:function(data){
	var param = {
		appId:$model.appId,
		engineName:$model.engineName,
		taskType:$model.taskList
    };
    $http.get(&quot;workflow/uniteconfig/loadconfig&quot;,{params:param}).success(function(data){
		if(data=='true'){
			$model.onlyTitle = true;
		}else{
			$model.onlyTitle = false;
		}
	});
	$view.fieldConfigGroup.refresh(param);
}</form:scripts>
