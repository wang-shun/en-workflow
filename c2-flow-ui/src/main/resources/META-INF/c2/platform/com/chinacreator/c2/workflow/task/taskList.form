todoTaskGrid_gridComplete:function(){
	var jqGrid = $view.todoTaskGrid.getDom();
	var param = {
		appId:$model.appId,
		engineName:$model.engineName,
		taskType:'todo'
    };
    $http.get(&quot;workflow/uniteconfig/loadcloumns&quot;,{params:param}).success(function(data,status,headers,config){
    	if(data.onlyTitle == '1'){
    		jqGrid.showCol('taskTitle');
			jqGrid.hideCol(data.showColumnIds);
			jqGrid.hideCol(data.hideColumnIds);
    	}else{
			jqGrid.showCol(data.showColumnIds);
			jqGrid.hideCol(data.hideColumnIds);
    		jqGrid.hideCol('taskTitle');
    	}
		angular.forEach(data.columns,function(column,n){
			var columnId = column.columnId;
			var columnName = column.columnName;
			jqGrid.setLabel(columnId, columnName, '', '');
		});
		if(angular.isUndefined($scope.remapCol)){
			jqGrid.remapColumns(data.remapColumns, true, false);
			$scope.remapCol=&quot;true&quot;;
		}
		$view.todoTaskGrid.recomputeWidth($view.todoTaskGrid.getDom().parents('.no-padding').width());
	});
},

