newField5_click:function(){
	 Modal.open(
	    'f/createWorkDatePop',
	    {},
	    function(){
		    $view.newGroup3.refresh();
	 });	
},

newField6_click:function(rowId,rowData,tableData){
    var selectNodes =$view.newGroup3.getSelectDatas();
	if(angular.isArray(selectNodes) &amp;&amp; selectNodes.length==1){
	    var node = selectNodes[0];
		Modal.open(
		    'f/editWorkDatePop',
		    {workId:node.workId,pmEndTime:node.pmEndTime,pmBeginTime:node.pmBeginTime
		    ,amEndTime:node.amEndTime,amBeginTime:node.amBeginTime
		    ,beginDate:node.beginDate,endDate:node.endDate,remark:node.remark},
		    function(){
			    $view.newGroup3.refresh();
		 });
	}else{
		Messenger.post({
			    'message': &quot;请选择一条记录！&quot;,
			    'type': 'error',
			});
	}
     
},

newField7_click:function(rowId,rowData,tableData){
	var selectNodes =$view.newGroup3.getSelectDatas();
	if(angular.isArray(selectNodes) &amp;&amp; selectNodes.length>0){
	   var workId = &quot;&quot;;
	   var workRemark = &quot;&quot;;
	   var arr = [];
	   angular.forEach(selectNodes, function (value, key) {
			workId += value.workId + &quot;,&quot;;
			workRemark += value.remark+&quot;,&quot;;
			arr.push(value.workId);
		});
		
		Modal.openDelConfirm({message:&quot;是否确认删除作息时间?&lt;br>待删除作息时间：&quot;+workRemark.substring(0, workRemark.length-1)},function(){
			var obj = {};
			obj = {
				params:{
					workIds:arr
				}
			};
			$http.get(&quot;ws/deleteWorkDates&quot;,obj).success(function(data){
				Messenger.post({
				    'message': &quot;批量删除成功！&quot;,
				    'type': 'success',
					});
				 $view.newGroup3.refresh();
				});
		});
		
	}else{
		Messenger.post({
			    'message': &quot;请选择需要删除的记录！&quot;,
			    'type': 'error',
			});
	}
}</form:scripts>
