                      <template:set param="iconColor" value="orange"/>
                      <template:hide param="click" value="true"/>
                    </template:case>
                    <template:case value="refrash">
                      <template:set param="icon" value="fa-refresh"/>
                      <template:set param="iconColor" value="green"/>
                      <template:hide param="click" value="true"/>
                    </template:case>
